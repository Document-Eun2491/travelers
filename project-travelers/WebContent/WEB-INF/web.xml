<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>project-travelers</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>



  <!-- userFrontCotroller로 매핑-->
    <servlet>
  	<servlet-name>User</servlet-name>
  	<servlet-class>com.app.user.UserFrontController</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>User</servlet-name>
  	<url-pattern>*.user</url-pattern>
  </servlet-mapping>


  <!-- mypageFrontCotroller로 매핑-->
    <servlet>
  	<servlet-name>MyPage</servlet-name>
  	<servlet-class>com.app.mypage.MyPageFrontController</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>MyPage</servlet-name>
  	<url-pattern>*.myPage</url-pattern>
  </servlet-mapping>

  <!-- AdminFrontCotroller로 매핑-->
    <servlet>
  	<servlet-name>Admin</servlet-name>
  	<servlet-class>com.app.admin.AdminFrontController</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>Admin</servlet-name>
  	<url-pattern>*.admin</url-pattern>
  </servlet-mapping>

  <!-- tripBoardFrontCotroller로 매핑-->
    <servlet>
  	<servlet-name>TripBoard</servlet-name>
  	<servlet-class>com.app.tripBoard.TripBoardFrontController</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>TripBoard</servlet-name>
  	<url-pattern>*.tripBoard</url-pattern>
  </servlet-mapping>

  <!-- ItemBoardFrontCotroller로 매핑-->
    <servlet>
  	<servlet-name>ItemBoard</servlet-name>
  	<servlet-class>com.app.itemBoard.ItemBoardFrontController</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>ItemBoard</servlet-name>
  	<url-pattern>*.itemBoard</url-pattern>
  </servlet-mapping>

  <!-- RouteBoardFrontCotroller로 매핑-->
    <servlet>
  	<servlet-name>RouteBoard</servlet-name>
  	<servlet-class>com.app.routeBoard.RouteBoardFrontController</servlet-class>
  </servlet>




  <servlet-mapping>
  	<servlet-name>RouteBoard</servlet-name>
  	<url-pattern>*.routeBoard</url-pattern>
  </servlet-mapping>



</web-app>